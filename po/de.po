#: data/com.github.p3732.OS-Installer.desktop.in:3
msgid "OS-Installer"
msgstr "OS-Installer"

#: data/com.github.p3732.OS-Installer.desktop.in:4
msgid "Install an Operating System"
msgstr "Installiere ein Betriebssystem"

#: data/com.github.p3732.OS-Installer.appdata.xml.in:6
msgid "Install an OS onto your system."
msgstr "Installiere ein Betriebssystem auf dein System."

#. Button label to confirm disk deletion
#. Button label for confirming timezone and formats choice
#: data/resources/ui/pages/confirm.ui:12 data/resources/ui/pages/locale.ui:147
msgid "_Confirm"
msgstr "_Bestätigen"

#. First part of confirmation warning, followed by the disk name and then 'will be deleted'
#: data/resources/ui/pages/confirm.ui:54
msgid "All data on"
msgstr "Alle Dateien auf"

#. See 'All data on'
#: data/resources/ui/pages/confirm.ui:83
msgid "will be deleted"
msgstr "werden gelöscht werden"

#. Button label to open disk management tool. Underscore can not be for same as for 'Reload'
#: data/resources/ui/pages/disk.ui:89
msgid "_Manage Disks"
msgstr "_Laufwerke verwalten"

#. Button label to reload current (disk) page. Underscore can not be for same as for 'Manage Disks'
#: data/resources/ui/pages/disk.ui:109
msgid "_Reload"
msgstr "_Neu Laden"

#. Informative label after successful installation
#: data/resources/ui/pages/done.ui:22
msgid "Installation complete"
msgstr "Installation vollständig"

#. Button label to restart system directly after successful installation. Underscore can not be for same letter as 'Restart Later'.
#: data/resources/ui/pages/done.ui:32
msgid "_Restart System"
msgstr "System _Neustarten"

#. Button label to delay restart after successful installation. Underscore can not be for same letter as 'Restart System'.
#: data/resources/ui/pages/done.ui:50
msgid "Restart _Later"
msgstr "_Später Neustarten"

#. Used with option to enable disk encryption
#: data/resources/ui/pages/encrypt.ui:31
msgid "Encrypt Installation"
msgstr "Installation verschlüsseln"

#. Description of entry field for pin/password used for disk encryption
#: data/resources/ui/pages/encrypt.ui:112
msgid "Encryption PIN"
msgstr "Verschlüsselungs-PIN"

#. Placeholder for disk encryption pin/password field
#: data/resources/ui/pages/encrypt.ui:127
msgid "PIN"
msgstr "PIN"

#. Warning label for disk encryption
#: data/resources/ui/pages/encrypt.ui:166
msgid ""
"Memorize this PIN well.\n"
"Without it you will not be able to access your files."
msgstr ""
"Präge dir diesen PIN gut ein.\n"
"Ohne ihn wirst du nicht auf deine Dateien zugreifen können."

#. Simple label used during Installation
#: data/resources/ui/pages/install.ui:20
msgid "Installing"
msgstr "Installation läuft"

#. Hover information on toggle button that shows terminal output during installation.
#: data/resources/ui/pages/install.ui:83
msgid "Show Terminal Output"
msgstr "Terminal-Ausgabe anzeigen"

#. Button label for starting opening the wifi settings
#: data/resources/ui/pages/internet.ui:12
msgid "_Setup Internet Connection"
msgstr "_Internetverbindung einrichten"

#. Used as placeholder in a entry field to try out a keyboard layout
#: data/resources/ui/pages/keyboard_layout.ui:81
msgid "Try Your Keyboard Layout Here"
msgstr "Teste deine Tastaturbelegung hier"

#. Label presented next to the current Timezone
#: data/resources/ui/pages/locale.ui:43
msgid "Timezone"
msgstr "Zeitzone"

#. Label presented next to the current Formats
#: data/resources/ui/pages/locale.ui:97
msgid "Formats"
msgstr "Formate"

#. Simple label used when restarting
#: data/resources/ui/pages/restart.ui:21
msgid "Restarting"
msgstr "Neustarten"

#. Informative label above a list of software that can be selected for installation
#: data/resources/ui/pages/software.ui:44
msgid "Select Additional Software to Install"
msgstr "Wähle zusätzliche Programme zur Installation aus"

#. Used with entry field for User's name
#: data/resources/ui/pages/user.ui:39
msgid "Name"
msgstr "Name"

#. Placeholder in user name entry field
#: data/resources/ui/pages/user.ui:56
msgid "Enter Your Name"
msgstr "Gebe deinen Namen ein"

#. Used with option to enable automatic login of user upon booting
#: data/resources/ui/pages/user.ui:98
msgid "Automatic Login"
msgstr "Automatisch Anmelden"

#. Used with entry field for setting a password for new user
#: data/resources/ui/pages/user.ui:148
msgid "Password"
msgstr "Passwort"

#. Placeholder in password entry field
#: data/resources/ui/pages/user.ui:162
msgid "Enter a Password"
msgstr "Gebe ein Passwort ein"

#. Shown if no name can be determined for a disk.
#: data/resources/ui/widgets/disk_row.ui:47
msgid "Unknow Disk"
msgstr "Unbekanntes Laufwerk"

#. Label to go back to overview of all disks
#: data/resources/ui/widgets/disk_back_row.ui:40
msgid "Use Different Disk"
msgstr "Nutze gesamtes Laufwerk"

#. Used to navigate to a list of all keyboard layouts
#: data/resources/ui/widgets/keyboard_layout_back_row.ui:41
msgid "Show All Layouts"
msgstr "Zeige alle Tastaturbelegungen"

#. Label to pick one out of a list of partitions. Followed by a partition name or a number if the partition has no name.
#: data/resources/ui/widgets/partition_row.ui:37
msgid "Use Partition"
msgstr "Nutze Partition"

#. Informative label with list of continents and countries
#: data/resources/ui/widgets/timezone_chooser.ui:14
msgid "Select a Location"
msgstr "Wähle einen Standort"

#. Label for installing on the whole selected disk
#: data/resources/ui/widgets/whole_disk_row.ui:33
msgid "Use Whole Disk"
msgstr "Nutze gesamtes Laufwerk"

